{
  "version": 2,
  "builds": [{
      "src": "preclinic/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
  }],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "preclinic/wsgi.py"
      }
  ],
  "rewrites": [
    {"source": "/(.*)", "destination": "/"}
  ],
  "outputDirectory": "staticfiles",
  "env": {
    "DATABASE_URL": "postgres://default:${DATABASE_PASSWORD}@ep-autumn-lab-a4by5dat.us-east-1.aws.neon.tech:5432/verceldb?sslmode=require"
  }
}